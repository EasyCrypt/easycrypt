    return true;
  }
  
  proc find(bc : bindconf, pk0 : pk_t, pk1 : pk_t) : ctxt_t * ctxt_t = {
    var b : bool;
    var c0, c1 : ctxt_t;

    (c0, c1) <@ A(O0, O1).find(K_Binds_PK, pk0, pk1);
    
    return (c0, c1);
  }
}. 


